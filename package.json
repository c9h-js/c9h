{
  "name": "c9h",
  "version": "1.0.0-next.1",
  "description": "Zero-config config for Node.js",
  "keywords": [
    "config",
    "c9h",
    "environment",
    "variables",
    "env-var",
    "envvar",
    "configuration",
    "typescript"
  ],
  "homepage": "https://c9h.carr.sh/",
  "bugs": "https://github.com/lukecarr/c9h/issues",
  "repository": "lukecarr/c9h",
  "license": "MIT",
  "author": "Luke Carr <me+oss@carr.sh> (https://carr.sh/)",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./taps": {
      "import": "./dist/taps.mjs",
      "require": "./dist/taps.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "test": "jest",
    "release": "yarn test && yarn build && npx release-it",
    "lint:eslint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "lint:prettier": "prettier *.md **/*.md *.json **/*.yml --write",
    "lint": "yarn lint:eslint && yarn lint:prettier"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ],
          "paths": [
            "src"
          ]
        }
      }
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {},
    "ignorePatterns": [
      "dist"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "junit"
  },
  "dependencies": {
    "@iarna/toml": "^3.0.0",
    "args-parser": "^1.3.0",
    "ini": "^2.0.0",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.0.1",
    "@types/ini": "^1.3.31",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
    "siroc": "^0.14.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
